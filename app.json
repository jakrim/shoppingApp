{
  "expo": {
    "name": "Jesse Krim - Shopping App",
    "slug": "jessekrim-shopping-app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android"],
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/shop-cart-icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 3000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.jessekrim.shopping-app",
      "buildNumber": "1.2.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.jessekrim.shopping_app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/shop-cart-icon.png",
        "backgroundColor": "#171717"
      },
      "permissions": ["WRITE_EXTERNAL_STORAGE"]
    },
    "description": "A shopping app built with React Native"
  }
}
